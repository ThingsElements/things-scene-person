{
  "name": "@things-scene/person",
  "version": "2.0.5",
  "description": "An element providing a starting point for your own reusable Polymer elements.",
  "main": "src/index.js",
  "things-scene": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "watchify src/*.js -o things-scene-person.js src/index.js & polyserve",
    "build": "browserify src/index.js -o things-scene-person.js && uglifyjs things-scene-person.js -o things-scene-person-min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThingsElements/things-scene-person.git"
  },
  "author": "",
  "license": "http://things-scene.hatiolab.com/EULA/",
  "bugs": {
    "url": "https://github.com/ThingsElements/things-scene-person/issues"
  },
  "homepage": "https://github.com/ThingsElements/things-scene-person#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "@hatiolab/things-shell": "^0.3.0"
  },
  "dependencies": {
    "@hatiolab/things-scene": "^2.2.36"
  }
}
